      <Monitoring Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Monitoring</Alias>
        <Columns isList="true" count="19">
          <value>Id,System.Int32</value>
          <value>Fullname,System.String</value>
          <value>IdentityNumber,System.String</value>
          <value>ContractNumber,System.String</value>
          <value>ContractDate,System.DateTime</value>
          <value>CollateralType,System.String</value>
          <value>LoanPeriod,System.Int32</value>
          <value>Category,System.String</value>
          <value>LoanPercent,System.Decimal</value>
          <value>CardType,System.String</value>
          <value>CountContracts,System.Int32</value>
          <value>GoodsDescription,System.String</value>
          <value>DebtCost,System.Int32</value>
          <value>DebtLeft,System.Decimal</value>
          <value>BranchName,System.String</value>
          <value>PercentPaymentType,System.Int32</value>
          <value>DelayDayCount,System.Int32</value>
          <value>LoanProfit,System.Decimal</value>
          <value>PenaltyProfit,System.Decimal</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Monitoring</Name>
        <NameInSource>ReportConnection</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT c.Id,
	UPPER(JSON_VALUE(c.ContractData,'$.Client.Fullname')) AS Fullname,
	UPPER(JSON_VALUE(c.ContractData,'$.Client.IdentityNumber')) AS IdentityNumber,
	c.ContractNumber,
	c.ContractDate,
	CASE c.CollateralType
	WHEN 10 THEN N'Золото'
	WHEN 20 THEN N'Авто'
	WHEN 30 THEN N'Товар'
	WHEN 40 THEN N'Спецтехника'
	END AS CollateralType,
	c.LoanPeriod,
cat.Name AS Category,
CAST(c.LoanPercent*30 AS DECIMAL(10,2)) AS LoanPercent,
	CASE JSON_VALUE(c.ContractData,'$.Client.CardType')
	WHEN 10 THEN N'Standard'
	WHEN 20 THEN N'Bronze'
	WHEN 30 THEN N'Silver'
	WHEN 40 THEN N'Gold'
	WHEN 50 THEN N'Platinum'
	END AS CardType,
(SELECT count(*) FROM Contracts WHERE DeleteDate IS NULL AND ClientId=c.ClientId AND ContractDate&lt;=@beginDate) as CountContracts,
	CASE c.CollateralType
	WHEN 10 THEN pos.Name
	WHEN 20 THEN CONCAT(UPPER(car.Mark),N' ',UPPER(car.Model),N' ',car.ReleaseYear,N'г. ',UPPER(car.TransportNumber))
	WHEN 30 THEN pos.Name
	WHEN 40 THEN CONCAT(car.Mark,N' ',car.Model,N' ',car.ReleaseYear,N' ',car.TransportNumber)
	END AS GoodsDescription,
	c.LoanCost as DebtCost,
cast((c.LoanCost-isnull((SELECT SUM(Cost) FROM ContractActions 
					JOIN ContractActionRows on ActionId=ContractActions.Id AND PaymentType=10
					WHERE DeleteDate IS NULL AND ContractActions.Date&lt;=@beginDate AND ContractId = c.Id AND ActionType in(20,30,40,60,80,90)),0)) AS decimal(10,0)) AS DebtLeft,
g.DisplayName AS BranchName,
c.PercentPaymentType,
	IIF((
			--ANNITY
	CASE	WHEN c.PercentPaymentType IN(30, 31) THEN DATEDIFF(DAY, DATEADD(MONTH, (
SELECT COUNT(*)
	FROM ContractActions ca
            WHERE ca.ContractId = c.Id
                AND ca.DeleteDate IS NULL
                AND ca.ActionType = 80
				AND ca.Date&lt;=@beginDate
        ) + 1, c.ContractDate), dbo.GETASTANADATE())
			--AS USUAL
			ELSE DATEDIFF(DAY,
IIF((SELECT COUNT(*) FROM contractactions 
	WHERE ContractId=c.Id 
	AND DeleteDate IS NULL
						AND ActionType=10
						AND Date&lt;=@beginDate)&gt;0,
DATEADD(DAY,CAST((SELECT TOP 1 JSON_VALUE(Data,'$.ProlongPeriod') FROM contractactions 
					WHERE ContractId=c.Id 
						AND DeleteDate IS NULL
						AND ActionType=10
						AND Date&lt;=@beginDate
				ORDER BY Date DESC) AS INT),
(SELECT TOP 1 DATE FROM contractactions 
					WHERE ContractId=c.Id 
						AND DeleteDate IS NULL
						AND ActionType=10
						AND Date&lt;=@beginDate
				ORDER BY Date DESC)),c.OriginalMaturityDate),
				@beginDate
			)
			END

			)&lt;=0,0,(
	CASE	WHEN c.PercentPaymentType IN(30, 31) THEN DATEDIFF(DAY, DATEADD(MONTH, (
SELECT COUNT(*)
	FROM ContractActions ca
            WHERE ca.ContractId = c.Id
                AND ca.DeleteDate IS NULL
                AND ca.ActionType = 80
				AND ca.Date&lt;=@beginDate
        ) + 1, c.ContractDate), dbo.GETASTANADATE())
			--AS USUAL
			ELSE DATEDIFF(DAY,
IIF((SELECT COUNT(*) FROM contractactions 
	WHERE ContractId=c.Id 
	AND DeleteDate IS NULL
						AND ActionType=10
						AND Date&lt;=@beginDate)&gt;0,
DATEADD(DAY,CAST((SELECT TOP 1 JSON_VALUE(Data,'$.ProlongPeriod') FROM contractactions 
					WHERE ContractId=c.Id 
						AND DeleteDate IS NULL
						AND ActionType=10
						AND Date&lt;=@beginDate
				ORDER BY Date DESC) AS INT),
(SELECT TOP 1 DATE FROM contractactions 
					WHERE ContractId=c.Id 
						AND DeleteDate IS NULL
						AND ActionType=10
						AND Date&lt;=@beginDate
				ORDER BY Date DESC)),c.OriginalMaturityDate),
				@beginDate
			)
			END
			)) AS DelayDayCount,
isnull((SELECT SUM(Cost) FROM ContractActions 
					JOIN ContractActionRows on ActionId=ContractActions.Id AND PaymentType=20
					WHERE DeleteDate IS NULL AND ContractId = c.Id AND ContractActions.Date&lt;=@beginDate AND ActionType in(10,20,30,40,60,80,90)),0) AS LoanProfit,
isnull((SELECT SUM(Cost) FROM ContractActions 
					JOIN ContractActionRows on ActionId=ContractActions.Id AND PaymentType=30
					WHERE DeleteDate IS NULL AND ContractId = c.Id AND ContractActions.Date&lt;=@beginDate AND ActionType in(10,20,30,40,60,80,90)),0) AS PenaltyProfit


FROM Contracts c
LEFT JOIN ContractPositions cp on cp.ContractId=c.Id
LEFT JOIN Cars car on car.Id=cp.PositionId and c.CollateralType=20
LEFT JOIN Machineries m on m.Id=cp.PositionId and c.CollateralType=40
LEFT JOIN Positions pos on pos.Id=cp.PositionId and c.CollateralType!=40 and c.CollateralType!=20
LEFT JOIN Categories cat on cat.id=cp.CategoryId
LEFT JOIN ContractActions ca on ca.ContractId=c.Id AND ca.Date&lt;@beginDate AND ca.ActionType in (20, 30, 40, 60, 90) AND ca.DeleteDate IS NULL
JOIN Groups g on g.Id=c.BranchId
WHERE	c.DeleteDate IS NULL
	AND c.ContractDate&lt;=@beginDate
	AND ca.Id IS NULL
	AND c.BranchId in ({branchIds})

ORDER BY BranchName, ContractDate</SqlCommand>
      </Monitoring>
